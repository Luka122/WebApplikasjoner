@model IEnumerable<NutritionModel>
@{
    ViewData["Title"] = "Nutrition";
}
<h1>@ViewData["Title"]</h1>
<table>
    <thead>
        <tr>
            <th>Food Item</th>
            <th>Calories</th>
            <th>Protein</th>
            <th>Fat</th>
            <th>Carbohydrates</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FoodItem</td>
                <td>@item.Calories</td>
                <td>@item.Protein</td>
                <td>@item.Fat</td>
                <td>@item.Carbohydrates</td>
            </tr>
        }
    </tbody>
</table>